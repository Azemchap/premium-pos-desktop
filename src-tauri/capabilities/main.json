{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "main-capability",
    "description": "Capability for the main window",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "core:path:default",
        "core:image:default",
        "fs:default",
        "fs:allow-read-file",
        "fs:allow-read-dir",
        "shell:allow-open",
        "notification:default",
        "sql:default"
    ]
}