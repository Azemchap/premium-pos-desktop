{
    "name": "premium-pos-desktop",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:web": "tsc && vite build --mode web",
        "preview": "vite preview",
        "preview:web": "vite preview --mode web",
        "tauri": "tauri",
        "tauri:dev": "tauri dev",
        "tauri:build": "tauri build",
        "seed": "echo Database will be seeded automatically on first run",
        "reset-db": "node -e \"const fs=require('fs'); const path=require('path'); try { fs.unlinkSync(path.join('src-tauri','target','debug','pos.db')); console.log('Database deleted successfully'); } catch(e) { console.log('Database file not found or already deleted'); }\"",
        "build:all": "pnpm build && pnpm tauri build",
        "build:windows": "pnpm build && pnpm tauri build --target x86_64-pc-windows-msvc",
        "build:linux": "pnpm build && pnpm tauri build --target x86_64-unknown-linux-gnu",
        "build:macos": "pnpm build && pnpm tauri build --target x86_64-apple-darwin"
    },
